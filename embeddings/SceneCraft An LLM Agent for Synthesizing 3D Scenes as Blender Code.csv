def score_4(locs):
import math
def distance(a, b):
return math.sqrt((a[’x’] - b[’x’]) **2 + (a[’y’] - b[’y’]) **2 + (a[’z’] - b[’z’]) **2)
# Rotation score (distance from sun)
rotation_score = sum(distance(locs[p], locs[’sun’]) for p in [’planet1’, ’planet2’, ’planet3’])
# Scaling score (size of planets)
scaling_score = abs(locs[’planet1’][’size’] - locs[’planet2’][’size’]) + abs(locs[’planet2’][’size’] - locs[’planet3’][’size’])
# Total score
total_score = rotation_score + scaling_score
return 1 - total_score / 100
